    "import numpy as np\n",
    "import torch as T\n",
    "\n",
    "def rescale_q_target(x, eps=0.01):\n",
    "    return np.sign(x) * (np.sqrt(np.abs(x) + 1) -1) + eps*x\n",
    "\n",
